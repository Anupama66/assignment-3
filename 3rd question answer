def procedure(string,target):
    words=string.split(" ")
    solution=[]
    for i in range(len(words)):
        if words[i]==target:
            solution.append(i)
        else len(solution)==0:
            return false
            return solution
string="we dont need no education we dont need no thought control no we dont"
print(procedure(string,"dont"))
assert(procedure(string,"dont"))
